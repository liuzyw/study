      "data": "<p>receipt_code &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; ———&gt; SerialNumber &nbsp; &nbsp;是\t验证券码\t<br>\nshopid &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;已有<br>\ndeal_id &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;有<br>\ndealgroup_id &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; 已有<br>\ndeal_title &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;? &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;<br>\ndeal_price &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;填充-- &nbsp; &nbsp;<br>\ndeal_marketprice &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;填充-- &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;<br>\nverify_account &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; ——&gt; &nbsp;<span style=\"background-color: rgb(52, 65, 52); font-family: Menlo; font-size: 9pt;\">accountId</span><span style=\"color: rgb(221, 221, 221);\">&nbsp;&nbsp; &nbsp; &nbsp;string\t否\t验证帐号&nbsp;</span></p><p>\nverify_type &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; ———&gt; &nbsp; &nbsp;channel &nbsp; &nbsp;是\t验证方式：商家后台验证；服务商验证等</p><pre style=\"background-color:#404040;color:#9bc28e;font-family:'Menlo';font-size:8.3pt;\"><span style=\"background-color:#46464d;\"><br></span></pre><pre style=\"background-color:#404040;color:#9bc28e;font-family:'Menlo';font-size:8.3pt;\"><span style=\"background-color:#46464d;\"><br></span></pre><pre style=\"background-color:#404040;color:#9bc28e;font-family:'Menlo';font-size:8.3pt;\"><span style=\"background-color:#46464d;\">TG_ReceiptVerifySuccessRecord</span></pre><pre style=\"background-color: rgb(64, 64, 64);\"><span style=\"color: rgb(185, 185, 185); font-family: Menlo; font-size: 8.3pt; font-weight: bold;\">RecordID             </span><span style=\"color: rgb(202, 158, 77); font-family: Menlo; font-size: 8.3pt; font-weight: bold;\">int auto_increment comment </span><font face=\"Menlo\"><span style=\"font-size: 11.066666603088379px;\"><b>‘</b></span></font><span style=\"color: rgb(52, 180, 52); font-family: Menlo; font-size: 8.3pt; font-weight: bold;\">PK' </span><span style=\"color: rgb(202, 158, 77); font-family: Menlo; font-size: 8.3pt; font-weight: bold;\">primary key</span><font color=\"#9bc28e\" face=\"Menlo\"><span style=\"font-size: 8.3pt;\">,<br></span></font><span style=\"color: rgb(185, 185, 185); font-family: Menlo; font-size: 8.3pt; font-weight: bold;\">ReceiptID            </span><span style=\"color: rgb(202, 158, 77); font-family: Menlo; font-size: 8.3pt; font-weight: bold;\">int not null comment </span><span style=\"color: rgb(52, 180, 52); font-family: Menlo; font-size: 8.3pt; font-weight: bold;\">'ReceiptID,券表主键'</span><font color=\"#9bc28e\" face=\"Menlo\"><span style=\"font-size: 8.3pt;\">,<br></span></font><span style=\"color: rgb(185, 185, 185); font-family: Menlo; font-size: 8.3pt; font-weight: bold;\">SerialNumber         </span><span style=\"color: rgb(202, 158, 77); font-family: Menlo; font-size: 8.3pt; font-weight: bold;\">varchar</span><font color=\"#9bc28e\" face=\"Menlo\"><span style=\"font-size: 8.3pt;\">(</span></font><span style=\"color: rgb(249, 113, 187); font-family: Menlo; font-size: 8.3pt;\">50</span><font color=\"#9bc28e\" face=\"Menlo\"><span style=\"font-size: 8.3pt;\">) </span></font><span style=\"color: rgb(202, 158, 77); font-family: Menlo; font-size: 8.3pt; font-weight: bold;\">not null comment </span><span style=\"color: rgb(52, 180, 52); font-family: Menlo; font-size: 8.3pt; font-weight: bold;\">'团购序列号</span><font face=\"Menlo\"><span style=\"font-size: 11.066666603088379px;\"><b>’</b></span></font><font color=\"#9bc28e\" face=\"Menlo\"><span style=\"font-size: 8.3pt;\">,         --<br></span></font><span style=\"color: rgb(185, 185, 185); font-family: Menlo; font-size: 8.3pt; font-weight: bold;\">DealID               </span><span style=\"color: rgb(202, 158, 77); font-family: Menlo; font-size: 8.3pt; font-weight: bold;\">int not null comment </span><span style=\"color: rgb(52, 180, 52); font-family: Menlo; font-size: 8.3pt; font-weight: bold;\">'套餐ID</span><font face=\"Menlo\"><span style=\"font-size: 11.066666603088379px;\"><b>’</b></span></font><font color=\"#9bc28e\" face=\"Menlo\"><span style=\"font-size: 8.3pt;\">,                    --<br></span></font><span style=\"color: rgb(185, 185, 185); font-family: Menlo; font-size: 8.3pt; font-weight: bold;\">DealGroupID          </span><span style=\"color: rgb(202, 158, 77); font-family: Menlo; font-size: 8.3pt; font-weight: bold;\">int not null comment </span><span style=\"color: rgb(52, 180, 52); font-family: Menlo; font-size: 8.3pt; font-weight: bold;\">'团购ID</span><font face=\"Menlo\"><span style=\"font-size: 11.066666603088379px;\"><b>’</b></span></font><font color=\"#9bc28e\" face=\"Menlo\"><span style=\"font-size: 8.3pt;\">,                    --<br></span></font><span style=\"color: rgb(185, 185, 185); font-family: Menlo; font-size: 8.3pt; font-weight: bold;\">AccountID            </span><span style=\"color: rgb(202, 158, 77); font-family: Menlo; font-size: 8.3pt; font-weight: bold;\">int not null comment </span><span style=\"color: rgb(52, 180, 52); font-family: Menlo; font-size: 8.3pt; font-weight: bold;\">'操作人ID</span><font face=\"Menlo\"><span style=\"font-size: 11.066666603088379px;\"><b>’</b></span></font><font face=\"Menlo\"><font color=\"#9bc28e\"><span style=\"font-size: 8.3pt;\">,                  </span></font><span style=\"font-size: 11.066666603088379px;\">—-?</span><font color=\"#9bc28e\"><span style=\"font-size: 8.3pt;\"><br></span></font></font><span style=\"color: rgb(185, 185, 185); font-family: Menlo; font-size: 8.3pt; font-weight: bold;\">ShopID       